{
  "name": "aws-kms-provider",
  "version": "0.3.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/odanado/aws-kms-provider"
  },
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "lib",
    "package.json",
    "yarn.lock"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "run-jest": "jest --detectOpenHandles --forceExit --coverage",
    "prebuild": "rm -rf dist lib",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "run-p test:*",
    "test:jest": "yarn run-jest ./src",
    "test:coverage": "yarn test:jest --coverage",
    "test:lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "e2e": "yarn run-jest ./test",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/asn1js": "^2.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/ethereum-protocol": "^1.0.1",
    "@types/jest": "^26.0.23",
    "@types/keccak": "^3.0.1",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@types/secp256k1": "^4.0.2",
    "@types/web3-provider-engine": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^25.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "ts-jest": "^25.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "web3": "^1.4.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.19.0",
    "@open-rpc/client-js": "^1.7.0",
    "asn1js": "^2.1.1",
    "bn.js": "^5.2.0",
    "ethereumjs-common": "^1.5.2",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-util": "^7.0.10",
    "keccak": "^3.0.1",
    "secp256k1": "4.0",
    "web3-provider-engine": "^16.0.1"
  },
  "peerDependencies": {}
}
